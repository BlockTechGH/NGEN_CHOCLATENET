@model NGEN_CRM.Models.Chat
@{
    Layout = null;
}
<!DOCTYPE html>
<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">

    <title>NGEN</title>
    <link rel="apple-touch-icon" href="https://i.imgur.com/QRAUqs9.png">
    <link rel="shortcut icon" href="https://i.imgur.com/QRAUqs9.png">
    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/normalize.css@8.0.0/normalize.min.css">
    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap@4.1.3/dist/css/bootstrap.min.css">
    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/font-awesome@4.7.0/css/font-awesome.min.css">
    <link rel="stylesheet" href="https://cdn.jsdelivr.net/gh/lykmapipo/themify-icons@0.1.2/css/themify-icons.css">
    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/pixeden-stroke-7-icon@1.2.3/pe-icon-7-stroke/dist/pe-icon-7-stroke.min.css">
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/flag-icon-css/3.2.0/css/flag-icon.min.css">
    <link rel="stylesheet" href="../assets/css/cs-skin-elastic.css">
    <link rel="stylesheet" href="../assets/css/style.css">
    <link href="https://cdn.jsdelivr.net/npm/chartist@0.11.0/dist/chartist.min.css" rel="stylesheet">
    <link href="https://cdn.jsdelivr.net/npm/jqvmap@1.5.1/dist/jqvmap.min.css" rel="stylesheet">
    <link href="http://cdn.rawgit.com/davidstutz/bootstrap-multiselect/master/dist/css/bootstrap-multiselect.css" rel="stylesheet" type="text/css" />
    <style>
        span.stars, span.stars span {
            display: block;
            background-size: contain;
            background: url( 'data:image/svg+xml;base64,' ) 0 -21px repeat-x;
            width: 80px;
            height: 24px;
        }

            span.stars span {
                background-position: 0 0;
            }
    </style>
</head>
<body>
    <div class="content">
        <div class="animated fadeIn">
            <form class="form-validate form-horizontal" action="Create" method="post">
                <div class="row">
                    <div class="col-lg-12 card">
                        <div class="col-lg-12">

                            <div class="row" style="padding-top: 30px;padding-left: 12px;padding-right: 12px">
                                <div class="col-sm-6 col-lg-4">
                                    <div class="card text-white bg-flat-color-1">
                                        <div class="card-body">
                                            <div class="card-left pt-1 float-left">
                                                <h1 class="mb-0 fw-r">
                                                    <span class="count" style="font-weight:700">@ViewBag.TotalConv</span>
                                                </h1>
                                                <p class="text-light mt-1 m-0">Total Conversation</p>
                                            </div><!-- /.card-left -->
                                            <div class="card-right float-right text-right">
                                                <i class="icon fade-5 icon-lg pe-7s-menu" style="font-weight: 700"></i>
                                            </div><!-- /.card-right -->
                                        </div>
                                    </div>
                                </div>
                                <!--/.col-->
                                <div class="col-sm-6 col-lg-4">
                                    <div class="card text-white " style="background-color:#e6445a">
                                        <div class="card-body">
                                            <div class="card-left pt-1 float-left">
                                                <h1 class="mb-0 fw-r">
                                                    <span class="count" style="font-weight:700">@ViewBag.TotalMsg</span>
                                                </h1>
                                                <p class="text-light mt-1 m-0">Messages</p>
                                            </div><!-- /.card-left -->
                                            <div class="card-right float-right text-right">
                                                <i class="icon fade-5 icon-lg pe-7s-comment" style="font-weight: 700"></i>
                                            </div><!-- /.card-right -->
                                        </div>
                                    </div>
                                </div>
                                <!--/.col-->
                                <div class="col-sm-6 col-lg-4">
                                    <div class="card text-white bg-flat-color-2">
                                        <div class="card-body">
                                            <div class="card-left pt-1 float-left">
                                                <h1 class="mb-0 fw-r">
                                                    <span class="count" style="font-weight:700">@ViewBag.TotalAvgT</span>
                                                </h1>
                                                <p class="text-light mt-1 m-0">Avg.ResponseTime</p>
                                            </div><!-- /.card-left -->
                                            <div class="card-right float-right text-right">
                                                <i class="icon fade-5 icon-lg pe-7s-timer" style="font-weight: 700"></i>
                                            </div><!-- /.card-right -->
                                        </div>
                                    </div>
                                </div>
                                <!--/.col-->

                            </div>
                        </div>
                    </div>
                </div>
                <!-- /Widgets -->
                <div class="clearfix"></div>
                <!-- Agent Summary -->
                <div class="orders">
                    <div class="row">
                        <div class="col-sm-6 col-lg-6 card" style="padding:9px">
                            <div class="card-header" style="font-size: x-large;font-weight: 400;">
                                AGENT CHAT SUMMARY
                            </div>
                            <div class="card-body  card-block">
                                <div class="table-stats order-table ov-h">
                                    <table class="table">
                                        <thead>
                                            <tr>
                                                <th style="width:320px">Agent Name</th>
                                                <th style="width:320px">Converstion</th>
                                                <th style="width:200px">Messages </th>
                                                <th style="width:200px">Avg Resp Time </th>
                                                <th style="width:200px">Sup.Rating</th>

                                            </tr>
                                        </thead>
                                        <tbody>
                                            @foreach (var item in Model.ChatList)
                                            {
                                                <tr>
                                                    <td>@Html.DisplayFor(modelItem => item.AgentName)</td>
                                                    <td>@Html.DisplayFor(modelItem => item.TotalConversation)</td>
                                                    <td> @Html.DisplayFor(modelItem => item.Messages)</td>
                                                    <td> @Html.DisplayFor(modelItem => item.AvgRespTime)</td>
                                                    <td>  <span class="stars"> @item.SuprRating</span></td>
                                                </tr>
                                            }
                                        </tbody>
                                    </table>
                                </div> <!-- /.table-stats -->
                            </div>
                        </div>  <!-- /.col-lg-8 -->
                        <!-- /.Agent Summary -->
                        <!-- Q Summary -->
                        <div class="col-sm-6 col-lg-6 card" style="padding:9px">

                            <div class="card-header" style="font-size: x-large;font-weight: 400;">
                                CHANNEL  SUMMARY
                            </div>
                            <div class="card-body  card-block">
                                <div class="table-stats order-table ov-h">
                                    <table class="table ">
                                        <thead>
                                            <tr>
                                                <th>Channel</th>
                                                <th>Conversation </th>
                                                <th>Messages </th>
                                                <th>Avg.ResponseTime </th>
                                            </tr>
                                        </thead>
                                        <tbody>
                                            @if (Model.ChannelList != null)
                                            {
                                                foreach (var item in Model.ChannelList)
                                                {
                                                    <tr>
                                                        <td>@Html.DisplayFor(modelItem => item.ChannelName)</td>
                                                        <td>@Html.DisplayFor(modelItem => item.T_Conv)</td>
                                                        <td>@Html.DisplayFor(modelItem => item.T_Msg)  </td>
                                                        <td>@Html.DisplayFor(modelItem => item.T_AvgResp)  </td>
                                                    </tr>
                                                }
                                            }
                                        </tbody>
                                    </table>
                                </div> <!-- /.table-stats -->
                            </div>
                        </div> <!-- /.card -->
                    </div>  <!-- /.col-lg-8 -->

                </div>

                <!-- /Q Summary-->


            </form>
        </div><!-- .animated -->
    </div><!-- .content -->
    <div class="clearfix"></div>
    <!-- Footer -->
    <footer class="site-footer">
        <div class="footer-inner bg-white">
            <div class="row">
                <div class="col-sm-6">
                </div>
                <div class="col-sm-6 text-right">
                    <ul>
                        <li>
                            <a href="../Home/Contact"><i style="font-size:xx-large" class="menu-icon fa fa-home"></i> </a>
                        </li>
                    </ul>
                </div>
            </div>
        </div>
    </footer>
    <script type="text/javascript" src="http://ajax.googleapis.com/ajax/libs/jquery/1.8.3/jquery.min.js"></script>
    <script>
        $.fn.stars = function () {
            return $(this).each(function () {
                // Get the value
                var val = parseFloat($(this).html());
                // Make sure that the value is in 0 - 5 range, multiply to get width
                var size = Math.max(0, (Math.min(5, val))) * 20;
                // Create stars holder
                var $span = $('<span />').width(size);
                // Replace the numerical value with stars
                $(this).html($span);
            });
        }
        $(document).ready(function () {
            $(function () {
                $('span.stars').stars();
            });
        });
    </script>
</body>
</html>


