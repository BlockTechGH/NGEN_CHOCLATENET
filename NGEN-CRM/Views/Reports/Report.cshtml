                                                        <td> @Html.DisplayFor(modelItem => item.Inbound)</td>
                                                        <td> @Html.DisplayFor(modelItem => item.SLA)</td>
                                                        <td> @Html.DisplayFor(modelItem => item.Outbound)</td>
