                                                <td>@Html.DisplayFor(modelItem => item.Duration)</td>
                                                <td> @Html.DisplayFor(modelItem => item.CallType)</td>
                                                <td> @Html.DisplayFor(modelItem => item.Agent)</td>
                                                <td> @Html.DisplayFor(modelItem => item.QueueName)</td>
                                            </tr>
                                                }
                                            }
                                        </tbody>
                                    </table>
                                </div> <!-- /.table-stats -->
                            </div>
                        </div> <!-- /.card -->
                    </div>  <!-- /.col-lg-8 -->


                </div>
            </div>

